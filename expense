def expense():
    expense_list = {
    "food":[],
    "housing":[],
    "clothing":[],
    "transportation":[],
    "entertainment":[],
    "saving":[],
}
    
#user input
    category = input("which expense:")
    items = input("what is your items:")
    price = float(input("how much do you cost:"))

    if category in expense_list:
        expense_list[category].append({
            "item": items,
            "price": price
        })
    else:
       print("enter category")
    
    return expense_list

expenses = expense()
print(expense)
